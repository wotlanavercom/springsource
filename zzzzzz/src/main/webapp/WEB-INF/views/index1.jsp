						if(btn01.hasClass('dim')) btn01.removeClass('dim');
						btn01.attr('href','javascript:saveCoupon(\''+cp_id+'\', \''+aci_id+'\', \''+coupon.cpUseFl+'\', \''+cp_type+'\');');
